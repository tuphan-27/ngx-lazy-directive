{
  "name": "ngx-lazy-directive",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "ng build ngx-lazy-directive",
    "publish-lib": "npm run build-lib && node ./updatePackageJson.js && cp -rf ./README.md ./dist/ngx-lazy-directive && cd ./dist/ngx-lazy-directive && npm publish",
    "release": "standard-version",
    "release-rc": "standard-version --prerelease rc",
    "release-major": "standard-version -r major",
    "release-minor": "standard-version -r minor",
    "release-fix": "standard-version -r patch",
    "release-dry": "standard-version  --dry-run -r patch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tuphan-27/ngx-lazy-directive.git"
  },
  "author": {
    "name": "Tu Phan",
    "email": "pt27122000@gmail.com"
  },
  "keywords": [
    "lazy load",
    "lazy load component",
    "directive",
    "angular",
    "ngx"
  ],
  "homepage": "https://github.com/tuphan-27/ngx-lazy-directive#readme",
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "edit-json-file": "^1.8.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.0",
    "standard-version": "^9.5.0",
    "typescript": "~4.8.2"
  }
}
